  "ValidFrom": "2021-12-06T00:00:00+01:00",
  "ValidTo": "2030-06-01T00:00:00Z",
  "AffectedFields": [
    "v.0",
    "v.0.dn",
    "v.0.dt",
    "v.0.mp"
  ],
  "Logic": {
    "if": [
      {
        "var": "payload.v.0"
      },
      {
        "if": [
          {
            "===": [
              {
                "var": "payload.v.0.mp"
              },
              "EU/1/20/1525"
            ]
          },
          true,
          {
            "if": [
              {
                ">": [
                  {
                    "var": "payload.v.0.dn"
                  },
                  2
                ]
              },
              true,
              {
                "not-before": [
                  {
                    "plusTime": [
                      {
                        "var": "external.validationClock"
                      },
                      0,
                      "day"
                    ]
                  },
                  {
                    "plusTime": [
                      {
                        "var": "payload.v.0.dt"
                      },
                      14,
                      "day"
                    ]
                  }
                ]
              }
            ]
          }
        ]
      },
      true
    ]
  }
