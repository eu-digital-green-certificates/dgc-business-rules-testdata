  "CertificateType": "Vaccination",
  "Description": [
    {
      "lang": "en",
      "desc": "Date of vaccination must be before the current time."
    }
  ],
  "ValidFrom": "2021-06-01T00:00:00Z",
  "ValidTo": "2021-07-10T00:00:00Z",
  "AffectedFields": [
    "v.0",
    "v.0.dt"
  ],
  "Logic": {
    "if": [
      {
        "var": "payload.v.0"
      },
      {
        "not-before": [
          {
            "plusTime": [
              {
                "var": "external.validationClock"
              },
              0,
              "day"
            ]
          },
          {
            "plusTime": [
              {
                "var": "payload.v.0.dt"
              },
              0,
              "day"
            ]
          }
        ]
      },
      true
    ]
  }
}