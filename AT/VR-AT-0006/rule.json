{
	"Identifier": "VR-AT-0006",
	"Type": "Acceptance",
	"Country": "AT",
	"Version": "1.0.0",
	"SchemaVersion": "1.0.0",
	"Engine": "CERTLOGIC",
	"EngineVersion": "0.7.5",
	"CertificateType": "Vaccination",
	"Description": [
		{
			"lang": "en",
			"desc": "If (total number of doses <= 1 (cb: check if/why == not working!) and (sequencenumber >= total number of doses) (AT: 'vollst. Immunisierung bei 1/1') Verification Datetime must be more than 22 days and less than 270 days after vaccination date (and vaccination date not in the future)."
		}
	],
	"ValidFrom": "2021-06-01T00:00:00Z",
	"ValidTo": "2030-06-01T00:00:00Z",
	"AffectedFields": [
		"v.0",
		"v.0.dn",
		"v.0.sd",
		"v.0.dt"
	],
	"Logic": {
		"if": [
			{
				"var": "payload.v.0"
			},
			{
				"if": [
					{
						"==": [
							{
								"var": "payload.v.0.sd"
							},
							1
						]
					},
					{
						"if": [
							{
								">=": [
									{
										"var": "payload.v.0.dn"
									},
									{
										"var": "payload.v.0.sd"
									}
								]
							},
							{
								"not-after": [
									{
										"plusTime": [
											{
												"var": "payload.v.0.dt"
											},
											22,
											"day"
										]
									},
									{
										"plusTime": [
											{
												"var": "external.validationClock"
											},
											0,
											"day"
										]
									},
									{
										"plusTime": [
											{
												"var": "payload.v.0.dt"
											},
											270,
											"day"
										]
									}
								]
							},
							true
						]
					},
					true
				]
			},
			true
		]
	}
}