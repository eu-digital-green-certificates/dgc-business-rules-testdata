{
   "Identifier":"VR-MT-0005",
   "Type":"Acceptance",
   "Country":"MT",
   "Version":"1.0.0",
   "SchemaVersion":"1.0.0",
   "Engine":"CERTLOGIC",
   "EngineVersion":"0.7.5",
   "CertificateType":"Vaccination",
   "Description":[
      {
         "lang":"en",
         "desc":"The vaccination schedule must be complete (e.g., 1/1, 2/2,3/3) and Verification Datetime must be more than 14 days and less than 270 days after vaccination date."
      }
   ],
   "ValidFrom":"2022-01-15T00:00:00Z",
   "ValidTo":"2030-06-01T00:00:00Z",
   "AffectedFields":[
      "v.0",
      "v.0.dn",
      "v.0.sd",
      "v.0.dt"
   ],
   "Logic":{
      "if":[
         {
            "var":"payload.v.0"
         },
         {
            "===":[
               {
                  "var":"payload.v.0.dn"
               },
               {
                  "var":"payload.v.0.sd"
               }
            ]
         },
         {
            "not-after":[
               {
                  "plusTime":[
                     {
                        "var":"payload.v.0.dt"
                     },
                     14,
                     "day"
                  ]
               },
               {
                  "plusTime":[
                     {
                        "var":"external.validationClock"
                     },
                     0,
                     "day"
                  ]
               },
               {
                  "plusTime":[
                     {
                        "var":"payload.v.0.dt"
                     },
                     270,
                     "day"
                  ]
               }
            ]
         },
         true
      ]
   }
}